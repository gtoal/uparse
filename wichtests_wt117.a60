b̲e̲g̲i̲n̲
    s̲w̲i̲t̲c̲h̲ PATH :=
        P1,
        P2;
    i̲n̲t̲e̲g̲e̲r̲
        N,
        I;
    I := 0;
    LABEL 1 :;
    I := I + 1;
    i̲f̲ I = 2 t̲h̲e̲n̲
        g̲o̲t̲o̲ STOP;
    N := 0;
    g̲o̲t̲o̲
        i̲f̲ P > 0 t̲h̲e̲n̲
            PATH[N]
        e̲l̲s̲e̲
            LABEL 1;
    P1 := P - 1;
    P2 := P - 1;
    STOP :;
